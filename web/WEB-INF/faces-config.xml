<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
        <resource-bundle>
            <base-name>/com.BatallaNavalArboles.utilidades/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/BatallaNaval/paginabienvenido.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ingresar</from-outcome>
            <to-view-id>/BatallaNaval/accesoplataforma.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/BatallaNaval/accesoplataforma.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/BatallaNaval/crearjugador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>jugador2</from-outcome>
            <to-view-id>/BatallaNaval/iniciotablerojugados.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>jugador1</from-outcome>
            <to-view-id>/BatallaNaval/iniciotablerojugauno.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/BatallaNaval/iniciotablerojugauno.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tabju1</from-outcome>
            <to-view-id>/BatallaNaval/tablerosdisparos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/BatallaNaval/iniciotablerojugados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tabju2</from-outcome>
            <to-view-id>/BatallaNaval/tablerosdisparos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>


